<div class="panel-footer">
  <div id="site-info">
    <span class='author'>
  <% if (config.author){ %>
  &copy; <%= UserConfig.SiteDate %> <%= config.author %>
  <% } else { %>
  &copy; <%= UserConfig.SiteDate %> <%= UserConfig.title %>
  <% } %>
    &nbsp;&nbsp;</span>
  <% if (UserConfig.Beian) {%><a href='http://www.miibeian.gov.cn/'><%-UserConfig.Beian%></a></span><%}%>
  <% if (UserConfig.analytics && UserConfig.analytics.provider) {
       var provider = UserConfig.analytics.provider.split(',');
       provider.forEach(function(t){
     %><span id='analytics-<%-t%>'><%- partial('../_analytics/'+t,{mod_info:UserConfig.analytics[t]}) -%></span><%
       })
  }%>
  </div>
